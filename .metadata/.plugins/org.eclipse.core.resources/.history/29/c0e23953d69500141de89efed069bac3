#include "DriveSystem.h"

DriveSystem::DriveSystem(){

	stick  = new Joystick(0)
	stickTwo = new Joystick(1);
	talZeroEnco = new DigitalInput(5);
	talTwoEnco = new DigitalInput(6); //Change inputs for both of these.
	talZero = new Talon(0);
	talTwo = new Talon(2);
	drive = new RobotDrive(talZero, talTwo);

}

DriveSystem::~DriveSystem(){

	delete stick;
	delete stickTwo;
	delete talZeroEnco;
	delete talTwoEnco;
	delete talZero;
	delete talTwo;
	delete drive;

}

